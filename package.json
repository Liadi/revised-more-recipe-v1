{
  "name": "more-recipe",
  "version": "1.0.0",
  "description": "One place to share recipes and lot more",
  "main": "app.js",
  "scripts": {
    "heroku-postbuild": "npm run migrate",
    "start": "babel-node bin/www",
    "startdev": "nodemon --exec babel-node bin/www",
    "test:local": "cross-env NODE_ENV=test_local&& npm run migrate:undo&& npm run migrate&& nyc mocha server/test/test.js --compilers js:babel-core/register",
    "test:db": "set NODE_ENV=test && sequelize db:migrate:undo:all&&sequelize db:migrate && cross-env NODE_ENV=test nyc mocha server/test/test.js", 
    "test": "mocha --compilers js:babel-core/register test/test.js",
    "migrate": "sequelize db:migrate",
    "migrate:undo": "sequelize db:migrate:undo:all",
    "coverage": "node istanbul cover _mocha -- -R spec",
    "coveralls": "nyc npm test&& nyc report --reporter-text-lcov | coveralls"
  },
  "engines": {
    "node": "6.11.2",
    "npm": "3.10.10"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Liadi/more-recipe.git"
  },
  "nyc": {
    "require": [
      "babel-register"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "sourceMap": false,
    "instrument": false
  },


  "keywords": [
    "Javascript",
    "express",
    "restful-api"
  ],
  "author": "Liadi",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Liadi/more-recipe/issues"
  },
  "homepage": "https://github.com/Liadi/more-recipe#readme",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "bcryptjs": "^2.4.3", 
    "body-parser": "^1.17.2",
    "chai": "^4.1.2",
    "crypto": "^1.0.1",
    "express": "^4.15.4",
    "express-session":"^1.15.5",
    "mocha": "^3.5.0",
    "jsonwebtoken": "^7.4.3",
    "morgan": "^1.8.2",
    "pg": "^7.2.0",
    "pg-hstore": "^2.3.2",
    "sequelize": "^4.8.0",
    "sequelize-cli": "^2.8.0",
    "supertest": "^3.0.0"
  },
  "devDependencies": {
    "chai": "^4.1.1",
    "coveralls": "^2.13.1",
    "cross-env": "^5.0.5",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "eslint": "^4.6.1",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.3.0",
    "mocha": "^3.5.0",
    "nodemon": "^1.12.0",
    "istanbul": "^0.4.5",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^11.2.1",
    "supertest": "^3.0.0"

  }
}
